<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>
    <transfer name="AssetAddedMessage" strict="true">
        <property name="tenantUuid" dataBuilderRule="unique()->sentence(2)"/>
        <property name="appUuid" dataBuilderRule="unique()->sentence(2)"/>
        <property name="assetUuid" dataBuilderRule="unique()->sentence(2)"/>
        <property name="assetName" dataBuilderRule="unique()->sentence(2)"/>
        <property name="assetContent" dataBuilderRule="unique()->sentence(2)"/>
        <property name="slotKey" dataBuilderRule="unique()->sentence(2)"/>
        <property name="stores" />
    </transfer>

    <transfer name="CmsSlotCriteria">
        <property name="keys" type="string[]" singular="key"/>
    </transfer>
</transfers>
